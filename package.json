{
  "name": "foam-capture-action",
  "version": "1.0.0",
  "private": true,
  "description": "An action to capture information to your foam workspace",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "echo whooops!",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn lint && yarn run package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anglinb/foam-capture-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "foam"
  ],
  "author": "Brian Anglin <https://github.com/anglinb>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "eslint": "3.19.0",
    "foam-capture": "^0.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
